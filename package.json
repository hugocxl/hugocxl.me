{
  "name": "hugocorta.com",
  "version": "2.4.0",
  "private": true,
  "description": "My personal site",
  "homepage": "https://hugocorta.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/hcorta/hugocorta"
  },
  "scripts": {
    "dev": "next dev",
    "start": "next start",
    "build": "next build",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "setup:env": "npx vercel env pull .env.local",
    "lint": "eslint --ignore-path .gitignore .",
    "lint:check": "yarn lint",
    "lint:fix": "yarn lint --fix",
    "format": "prettier --ignore-path .gitignore .",
    "format:check": "yarn format --list-different",
    "format:fix": "yarn format --write",
    "type-check": "tsc",
    "type-check:check": "yarn type-check --noEmit",
    "static:validate": "yarn lint:check && yarn format:check && yarn type-check:check",
    "static:fix": "yarn lint:fix && yarn format:fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/server": "^11.10.0",
    "@mantine/core": "^5.7.2",
