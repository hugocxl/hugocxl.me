{
  "name": "hugocorta.com",
  "version": "2.7.3",
  "private": true,
  "description": "My personal site",
  "homepage": "https://hugocorta.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/hcorta/hugocorta"
  },
  "scripts": {
    "dev": "next dev",
    "start": "next start",
    "build": "next build",
    "test": "yarn test:unit && yarn test:e2e",
    "test:unit": "yarn jest --runInBand",
    "test:e2e": "yarn cypress:run",
    "cypress:run": "cypress run",
    "cypress:open": "cypress open",
    "lint": "eslint --ignore-path .gitignore .",
    "lint:check": "yarn lint",
    "lint:fix": "yarn lint --fix",
    "format": "prettier --ignore-path .gitignore .",
    "format:check": "yarn format --list-different",
    "format:fix": "yarn format --write",
    "type-check": "tsc",
    "type-check:check": "yarn type-check --noEmit",
    "static:validate": "yarn lint:check && yarn format:check && yarn type-check:check",
    "static:fix": "yarn lint:fix && yarn format:fix",
    "setup:env": "npx vercel env pull .env.local",
    "prepare": "husky install",
    "postversion": "git push && git push --tags",
    "update:release": "yarn version --allow-same-version=false --git-tag-version=true --new-version $v"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/server": "^11.10.0",
    "@mantine/core": "^6.0.0",
    "@mantine/ds": "^6.0.0",
    "@mantine/hooks": "^6.0.0",
    "@mantine/next": "^6.0.0",
    "@mantine/nprogress": "^6.0.0",
    "@mantine/prism": "^6.0.0",
    "@mantine/spotlight": "^6.0.0",
    "@notionhq/client": "^2.2.2",
    "@tabler/icons": "^1.111.0",
    "@testing-library/react": "^13.4.0",
    "@vercel/analytics": "^0.1.1",
    "cross-fetch": "^3.1.5",
    "next": "^13.0.0",
    "notion-client": "^6.15.6",
    "notion-types": "^6.15.6",
    "notion-utils": "^6.15.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.4.0",
    "react-notion-x": "^6.15.7",
    "react-tweet": "^2.0.0",
    "react-tweet-embed": "^2.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.7.8",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "cypress": "^12.2.0",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-config-standard-react": "^11.0.1",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.31.11",
    "husky": "^8.0.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "lint-staged": "^13.1.0",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
