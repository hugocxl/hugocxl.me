    "@mantine/hooks": "^5.7.2",
    "@mantine/next": "^5.7.2",
    "@mantine/nprogress": "^5.7.2",
    "@mantine/prism": "^5.7.2",
    "@mantine/spotlight": "^5.7.2",
    "@notionhq/client": "^2.2.2",
    "@tabler/icons": "^1.111.0",
    "@vercel/analytics": "^0.1.1",
    "cypress": "^12.2.0",
    "next": "^13.0.0",
